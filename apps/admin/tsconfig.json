{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@omm/supabase": [
        "../../packages/supabase/src"
      ],
      "@omm/supabase/*": [
        "../../packages/supabase/src/*"
      ],
      "@omm/types": [
        "../../packages/types/src"
      ],
      "@omm/types/*": [
        "../../packages/types/src/*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts"
  ],
  "exclude": [
    "node_modules"
  ]
}
